{
    "name": "jumbojett/openid-connect-php",
    "description": "Bare-bones OpenID Connect client",
    "license": "Apache-2.0",
    "repositories": [
        {
        "type": "vcs",
        "url": "https://github.com/FyraLabs/php-jwt",
        "no-api": true
        }
    ],
    "require": {
        "php": ">=7.0",
        "ext-json": "*",
        "ext-curl": "*",
        "phpseclib/phpseclib": "~3.0",
        "firebase/php-jwt": "dev-main as 6.5"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "yoast/phpunit-polyfills": "^1.0"
    },
    "archive" : {
        "exclude" : [
            ".*"
        ]
    },
    "autoload" : {
         "classmap": [ "src/"]
    }
}
